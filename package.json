{
	"name": "@nuogz/poseidon",
	"version": "8.2.0",
	"description": "Type-based config library",
	"author": "DanoR",
	"license": "LGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "github:nuogz/poseidon"
	},
	"private": false,
	"type": "module",
	"typesSource": [
		"node"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=14.18.1"
	},
	"main": "./index.js",
	"files": [
		"index.js",
		"lib/**",
		"locale/**",
		"**/*.d.ts"
	],
	"typings": "./index.d.ts",
	"scripts": {
		"emitDeclaration": "tsc ./index.js --declaration --emitDeclarationOnly --moduleResolution nodenext --module nodenext --target esnext --experimentalDecorators --strict --allowJs --jsx preserve"
	},
	"dependencies": {
		"@nuogz/i18n": "^3.3.1",
		"escape-string-regexp": "^5.0.0",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.24",
		"eslint": "^8.57.0"
	}
}
